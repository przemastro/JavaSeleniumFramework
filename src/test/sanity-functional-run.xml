<?xml version="1.0" encoding="UTF-8"?>

<suite name="Rule-CV Suite" verbose="1">
    <listeners>
        <listener class-name="com.testing.Reporter"/>
    </listeners>

    <test name="Daily Run" preserve-order="true">
        <classes>
            <class name="com.testing.Tests.Observations"/>
            <methods>
                <include name="addNewObservation"/>
                <include name="editUVBData"/>
                <include name="editObservation"/>
                <include name="removeObservation"/>
            </methods>
        </classes>
    </test>
</suite>